{
    "name": "Chrome Extension with Typescript",
    "version": "1.0.0.0",
    "manifest_versions": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./content/allpages.js"
            ]
        }
    ]
}